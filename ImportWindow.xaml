<Window x:Class="YukaLister.ImportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YukaLister"
        mc:Ignorable="d" Width="730" Height="330" Loaded="Window_Loaded" SourceInitialized="Window_SourceInitialized" Icon="アイコン/YukaListerアイコン_1.ico" ShowInTaskbar="False" ResizeMode="CanResizeWithGrip">
    <DockPanel>
        <DockPanel Margin="20,20,20,0" DockPanel.Dock="Top">
            <Button x:Name="ButtonAbort" Height="Auto" Content="中止 (_A)" Style="{StaticResource MaterialDesignRaisedLightButton}" Margin="0" DockPanel.Dock="Right" Width="100" Click="ButtonAbort_Click"/>
            <Label x:Name="LabelSrc" Content="-"/>
        </DockPanel>
        <DockPanel Margin="20,10,20,20">
            <TextBox x:Name="TextBoxLog" IsReadOnly="True" VerticalScrollBarVisibility="Auto" VerticalContentAlignment="Top"/>
        </DockPanel>
    </DockPanel>
</Window>
